{
  "name": "jobmatching-codex",
  "version": "1.0.0",
  "description": "User capsule upsert service",
  "type": "commonjs",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "index:create": "ts-node scripts/create-index.ts",
    "sync:main": "bash scripts/sync-main.sh"
  },
  "keywords": [
    "openai",
    "pinecone",
    "fastify"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "@prisma/client": "^7.2.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "openai": "^5.23.0",
    "pino": "^9.11.0",
    "prisma": "^7.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
